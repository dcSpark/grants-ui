    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/lib.mjs"
    }
  },
  "keywords": [],
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/dcSpark/grants-ui#readme",
  "repository": {
